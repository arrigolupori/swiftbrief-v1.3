generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  CS_SUPPORT
  SUPER_ADMIN
  ADMIN
  EDITOR
  WRITER
  VIEWER
}

model Organization {
  id String @id @default(cuid())
}

model User {
  id String @id @default(cuid())
}

model Brand {
  id String @id @default(cuid())
}

model Topic {
  id String @id @default(cuid())
}

model SubTopic {
  id String @id @default(cuid())
}

model Brief {
  id String @id @default(cuid())
}

model Link {
  id String @id @default(cuid())
}
